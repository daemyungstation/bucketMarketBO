                    S1.MEM_NM,
                    S1.CELL,
                    S1.PRD_MST_NM,
                    S1.ORD_MST_ORD_DT,
                    S1.JOIN_DT,
                    S1.KSTBIT,
                    S1.PAY,
                    S1.RMT_ORD_YN,
                    S1.RMT_ORD_PAY_DT
                FROM
                    (
                        SELECT
                            TT.MEM_NM,
                            TT.CELL,
                            TT.PRD_MST_NM,
                            TT.ORD_MST_ORD_DT,
                            TT.JOIN_DT,
                            TT.KSTBIT,
