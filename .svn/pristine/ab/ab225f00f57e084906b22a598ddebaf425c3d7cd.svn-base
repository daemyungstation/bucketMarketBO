                            TRO.RMT_ORD_YN,
                            TRO.RMT_ORD_PAY_DT,
                            TRO.RDP_MST_IDX,
                            TT.RDP_MST_IDX,
                            TT.ORD_MST_IDX
                        FROM
                            (
                                SELECT
                                    TOD.MEM_NM,
                                    TOD.CELL,
                                    CPM.PRD_MST_NM,
                                    TOM.ORD_MST_ORD_DT,
                                    VOM.JOIN_DT,
                                    VOM.KSTBIT,
                                    PRD_MST_RDP_DVD_PAY PAY,
                                    (#{searchCalculateYear} || LPAD(#{searchCalculateMonth}, 2, '0')) - SUBSTR(VOM.JOIN_DT, 0, 6) + 1 AS PAYMENTCOUNT,
                                    TOD.RDP_MST_IDX,
                                    TOD.ORD_MST_IDX
